{
  "name": "colorfuljs-node",
  "version": "1.0.1",
  "description": "Colorful.js is a javascript framework to simplify work with colors in the browser",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && npm run config",
    "config": "python setup.py",
    "build": "browserify src/main.js -o dist/browserfied.js && babel dist/browserfied.js -o dist/main.js && uglifyjs dist/main.js -o dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greencoder001/colorful.js.git"
  },
  "author": "Green_Lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greencoder001/colorful.js/issues"
  },
  "homepage": "https://github.com/greencoder001/colorful.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "browserify": "^17.0.0",
    "uglify-js": "^3.10.4"
  }
}
